    {
      "cell_type": "code",
      "source": [
        "model = Sequential()"
      ],
      "metadata": {
        "id": "E4XYJJWM--n3"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "from keras.models import Sequential\n",
        "from keras.layers import Dense\n",
        "\n",
        "model = Sequential()\n",
        "# First hidden layer with 4 units, input dimension is 4\n",
        "model.add(Dense(4, input_dim=4, activation='relu'))\n",
        "# Second hidden layer with 8 units\n",
        "model.add(Dense(8, activation='relu'))\n",
        "# Output layer with 1 unit, using sigmoid activation for binary classification\n",
        "model.add(Dense(1, activation='sigmoid'))\n"
      ],
      "metadata": {
        "id": "iXHIz3P08Id7"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "model.compile(loss='binary_crossentropy', optimizer='adam', metrics=['accuracy'])"
      ],
      "metadata": {
        "id": "MESnRddo_duY"
      },
      "execution_count": null,
      "outputs": []
